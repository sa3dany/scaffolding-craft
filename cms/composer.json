{
  "name": "company/www",
  "description": "",
  "require": {
    "craftcms/aws-s3": "1.2.11",
    "craftcms/cms": "^3.5.10",
    "craftcms/redactor": "2.8.1",
    "nystudio107/craft-seomatic": "3.3.18",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {}
  },
  "config": {
    "vendor-dir": "/usr/local/lib/craft",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
